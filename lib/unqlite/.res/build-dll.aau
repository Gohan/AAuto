io.open()
import tcc; 
vm = tcc( );

vm.addLib( 
	"user32",
	"kernel32",
	"gdi32"
) 

vm.onError = function(msg){
	vm.errorMsg = msg;
	io.print( msg )
}

 
vm.addFile("/unqlite.c")
vm.output( "/unqlite.dll", "" ) //编译C源码，生成DLL

if( vm.errorMsg ){
	io.print("编译出错,请升级您的TCC扩展库到0.9.26.02 以上版本")
}
else {
	io.print("已生成 unqlite.dll")
}
vm.close()

